    "matter-js": "^0.19.0",
    "minio": "^7.1.3",
    "passport": "^0.6.0",
    "passport-42": "^1.2.6",
    "passport-jwt": "^4.0.1",
    "passport-oauth2": "^1.7.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0",
